{
    "name": "brokeyourbike/zenith-bank-cib-api-client",
    "description": "Zenith Bank API Client for PHP (Corporate Internet Banking)",
    "keywords": [
        "api",
        "api-client",
        "zenith-bank",
        "brokeyourbike",
        "rest-api"
    ],
    "type": "library",
    "license": "MPL-2.0",
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\ZenithBankCIB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\ZenithBankCIB\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "brokeyourbike/http-client": "^1.0",
        "brokeyourbike/resolve-uri": "^1.0",
        "brokeyourbike/http-enums": "^2.0",
        "brokeyourbike/has-source-model": "^2.0",
        "brokeyourbike/data-transfer-object": "^0.2.0",
        "phpro/soap-client": "^1.7",
        "psr/http-message": "^1.1",
        "php-http/httplug": "^2.4",
        "php-http/message-factory": "^1.1",
        "php-http/discovery": "^1.19",
        "php-http/message": "^1.16",
        "php-http/client-common": "^2.7"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "nesbot/carbon": "^2.53",
        "laminas/laminas-code": "^4"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": false
        }
    }
}
