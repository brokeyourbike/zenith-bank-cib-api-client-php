{
    "name": "brokeyourbike/zenith-bank-cib-api-client",
    "description": "Zenith Bank API Client for PHP (Corporate Internet Banking)",
    "keywords": [
        "api",
        "api-client",
        "zenith-bank",
        "brokeyourbike",
        "rest-api"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\ZenithBankCIB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\ZenithBankCIB\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "phpro/soap-client": "^3",
        "guzzlehttp/psr7": "^2",
        "guzzlehttp/guzzle": "^7",
        "php-http/guzzle7-adapter": "^1",
        "php-http/message": "^1",
        "nesbot/carbon": "^2 || ^3"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": false
        }
    },
    "version": "0.5.3"
}
